include_rules

include files.mk
include libnxflat/files.mk
include libelf/files.mk

CFLAGS += -I$(TOPDIR)/sched

: foreach $(BINFMT_ASRCS) |> !as |> {objects}
: foreach $(BINFMT_CSRCS) |> !cc |> {objects}
: foreach $(BINFMT_CXXSRCS) |> !cxx |> {objects}
: {objects} |> !ar |> lib%d.a
